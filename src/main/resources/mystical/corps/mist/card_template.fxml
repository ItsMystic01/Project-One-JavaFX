<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox id="main_box" fx:id="main_box" stylesheets="@css/card_template.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="mystical.corps.mist.CardTemplate">
   <AnchorPane prefHeight="29.0" prefWidth="155.0" stylesheets="@css/card_template.css">
      <Label id="title" fx:id="box_title" alignment="CENTER" layoutX="4.0" layoutY="-3.0" prefHeight="29.0" prefWidth="155.0" styleClass="title" stylesheets="@css/card_template.css" text="Title" textAlignment="CENTER">
         <font>
            <Font name="Fira Code Bold" size="12.0" />
         </font></Label>
   </AnchorPane>
   <AnchorPane prefHeight="23.0" prefWidth="155.0">
      <Label id="starting_date" fx:id="box_starting_date" alignment="CENTER" layoutY="-5.0" prefHeight="36.0" prefWidth="80.0" text="Starting Date">
         <font>
            <Font name="HoloLens MDL2 Assets" size="12.0" />
         </font></Label>
      <Label id="ending_date" fx:id="box_ending_date" alignment="CENTER" layoutX="82.0" layoutY="-5.0" prefHeight="36.0" prefWidth="80.0" text="Ending Date">
         <font>
            <Font name="HoloLens MDL2 Assets" size="12.0" />
         </font></Label>
   </AnchorPane>
   <AnchorPane prefHeight="42.0" prefWidth="162.0">
      <Label id="priority" fx:id="box_priority" alignment="CENTER" contentDisplay="CENTER" layoutY="8.0" prefHeight="25.0" prefWidth="80.0" text="Priority">
         <font>
            <Font name="HoloLens MDL2 Assets" size="12.0" />
         </font></Label>
      <Label id="category" fx:id="box_category" alignment="CENTER" layoutX="81.0" layoutY="8.0" prefHeight="25.0" prefWidth="80.0" text="Category">
         <font>
            <Font name="HoloLens MDL2 Assets" size="12.0" />
         </font></Label>
   </AnchorPane>
   <AnchorPane prefHeight="11.0" prefWidth="155.0">
      <Button id="edit-button" fx:id="editButton" layoutX="15.0" layoutY="1.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="53.0" text="Edit" />
      <Button id="delete-button" fx:id="deleteButton" layoutX="95.0" layoutY="1.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="53.0" text="Delete" />
   </AnchorPane>
</VBox>
